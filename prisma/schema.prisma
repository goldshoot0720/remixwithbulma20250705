generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Udemy {
  id Int @id @default(autoincrement())
  course String @unique
  teacher String
  schedule String
}

model Bank {
  id Int @id @default(autoincrement())
  name String @unique
  saving Int 
}